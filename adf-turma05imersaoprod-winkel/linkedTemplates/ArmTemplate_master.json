{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-turma05imersaoprod-winkel"
		},
		"ls_azure_sqldb_powerquery_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_sqldb_powerquery'"
		},
		"ls_api_new_y_t_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.nytimes.com/svc/movies.v2"
		},
		"ls_azure_sqldb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-db-azure"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsturma05imersaowprod.dfs.core.windows.net"
		},
		"ls_filesystem_local_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "c:\\adf"
		},
		"ls_filesystem_local_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "analysis"
		},
		"ls_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-turma05im-prod-winkel.vault.azure.net/"
		},
		"ls_sqlserver_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-db-local"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-turma05imersaoprod-winkel_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_sqldb_powerquery_connectionString": {
						"value": "[parameters('ls_azure_sqldb_powerquery_connectionString')]"
					},
					"ls_api_new_y_t_properties_typeProperties_url": {
						"value": "[parameters('ls_api_new_y_t_properties_typeProperties_url')]"
					},
					"ls_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_azure_sqldb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"ls_filesystem_local_properties_typeProperties_host": {
						"value": "[parameters('ls_filesystem_local_properties_typeProperties_host')]"
					},
					"ls_filesystem_local_properties_typeProperties_userId": {
						"value": "[parameters('ls_filesystem_local_properties_typeProperties_userId')]"
					},
					"ls_sqlserver_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-turma05imersaoprod-winkel_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-turma05imersaoprod-winkel_ArmTemplate_0')]"
			]
		}
	]
}